[Unit]
Description=Local python webserver
After=network.target

[Service]
ExecStart=/home/pi/env/bin/python /home/pi/axis/cts-temperature/weblib/temperature_app.py /home/pi/axis/cts-temperature/conf/settings.json
Restart=always
RestartSec=5
User=pi
WorkingDirectory=/home/pi/axis/cts-temperature

[Install]
WantedBy=multi-user.target

